{
  "name": "expressjs-plus",
  "version": "1.0.2",
  "description": "A pluggable expansion to express js aimed at adding much needed features and helpers.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "compile": "rimraf lib/* && babel src --out-file lib/index.js",
    "build": "npm run compile",
    "generateDoc": "jsdoc2md -t jsdoc2md/readme.hbs src/*.js > readme.md"
  },
  "keywords": [
    "middleware",
    "express",
    "helper",
    "error",
    "handling",
    "error-handling"
  ],
  "author": "Abdulrahman Amri",
  "license": "MIT",
  "dependencies": {
    "errorhandler": "^1.4.3",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-runtime": "^6.11.6",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-git": "^3.10.0",
    "rimraf": "^2.5.4",
    "should": "^13.2.1",
    "sinon": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Amri91/expressjs-plus"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm run generateDoc",
        "npm run build",
        "git add .",
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
